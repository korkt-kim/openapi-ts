{
    "compilerOptions": {
      "declaration": true,
      "esModuleInterop": true,
      "module": "ESNext",
      "moduleResolution": "Bundler",
      "noImplicitOverride": true,
      "noImplicitReturns": true,
      "noUncheckedIndexedAccess": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "strict": true,
      "target": "ES2022",
      "useUnknownInCatchVariables": false
    }
  }