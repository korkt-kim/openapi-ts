{
    "$schema": "./node_modules/turbo/schema.json",
    "tasks": {
      "build": {
        "cache": false,
        "outputs": [
          ".next/**",
          "!.next/cache/**",
          ".output/**",
          ".vitepress/dist/**",
          "dist/**"
        ]
      },
      "dev": {
        "cache": false,
        "persistent": true
      },
      "test": {
        "cache": false,
        "dependsOn": []
      },
      "test:coverage": {
        "cache": false,
        "dependsOn": []
      },
      "test:watch": {
        "cache": false,
        "dependsOn": []
      }
    }
  }